{
  "name": "electron-puppeteer",
  "main": "index.js",
  "keywords": [
    "puppeteer",
    "electron"
  ],
  "author": "snyung",
  "license": "MIT",
  "description": "Electron+Puppeteer 작업해보기",
  "version": "0.0.1",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "electron-forge make -- --platform darwin",
    "make-linux": "electron-forge make -- --platform linux --targets deb",
    "make-window": "electron-forge make -- --platform win32"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "puppeteer": "^20.5.0",
    "puppeteer-core": "^20.5.0",
    "puppeteer-in-electron": "^3.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-dmg": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "electron": "^25.0.1"
  }
}